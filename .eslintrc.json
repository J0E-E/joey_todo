{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2023": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "globals": {
    "hbconfig": "readonly",
    "webAppChannel": "readonly",
    "webAppTriggerChannel": "readonly",
    "google": "readonly",
    "grecaptcha": "readonly",
    "HBH_BUILD": "readonly",
    "PRODUCTION": "readonly",
    "PLATFORM": "readonly",
    "SHIPPING_STORE_ID": "readonly",
    "FUNDRAISER_STORE_ID": "readonly",
    "LINE_OF_BUSINESS_COMPANY": "readonly",
    "LINE_OF_BUSINESS_FRANCHISE": "readonly",
    "LINE_OF_BUSINESS_SHIPPING": "readonly",
    "LINE_OF_BUSINESS_FUNDRAISER": "readonly",
    "COOKIE_TOKEN": "readonly",
    "COOKIE_CART": "readonly",
    "COOKIE_STATE": "readonly",
    "COOKIE_SESSION_STATE": "readonly",
    "COOKIE_STORE": "readonly",
    "COOKIE_PRIVACY": "readonly",
    "UTIL_URL": "readonly",
    "ITEM_URL": "readonly",
    "CART_URL": "readonly",
    "ITEMS_TTL": "readonly",
    "HBAPP": "readonly",
    "DEBOUNCE_ON": "readonly",
    "DEBOUNCE_TIMEOUT_MS": "readonly",
    "PUSH_STATE": "readonly",
    "API_VERSION": "readonly",
    "URL_BASE": "readonly",
    "URL_IMAGE_BASE": "readonly",
    "URL_FONTS_BASE": "readonly",
    "URL_CALIFORNIA": "readonly",
    "URL_API_BASE": "readonly",
    "API_TOKEN": "readonly",
    "USE_WEBSOCKET": "readonly",
    "FORCE_FLUTTER": "readonly",
    "USE_APPINSIGHTS": "readonly"
  },
  "ignorePatterns": ["backbone.js"],
  "rules": {
    "indent": ["error",4,{"SwitchCase": 1}],
    "quote-props":0,
    "no-var": 0,
    "no-param-reassign": 0,
    "no-trailing-spaces": 0,
    "prefer-arrow-callback": 0,
    "space-in-parens": 0,
    "no-multi-spaces":0,
    "space-before-function-paren":0,
    "space-before-blocks":0,
    "space-infix-ops":0,
    "no-plusplus":0,
    "no-tabs":0,
    "vars-on-top":0,
    "comma-style":0,
    "new-cap":0,
    "no-new":0,
    "padded-blocks": 0,
    "no-multiple-empty-lines": 0,
    "prefer-template": 0,
    "arrow-parens": 0,
    "arrow-spacing":0,
    "comma-dangle": 0,
    "no-unused-vars": 0,
    "no-alert":0,
    "object-shorthand":0,
    "prefer-destructuring":0,
    "no-multi-assign":0,
    "global-require":0,
    "dot-location":0,
    "quotes":0,
    "semi":0,
    "prefer-const":0,
    "eol-last":0,
    "default-case":0,
    "key-spacing":0,
    "object-curly-spacing":0,
    "object-curly-newline": 0,
    "operator-linebreak": 0,
    "brace-style": [ "error", "stroustrup"],
    "max-len": ["error",{ "code": 140 }],
    "prefer-rest-params": 0,
    "func-names": 0,
    "radix":0,
    "eqeqeq":0,
    "no-debugger": 0,
    "no-nested-ternary": 0,
    "import/no-dynamic-require": 0,
    "import/no-unresolved": 0,
    "import/extensions": ["error", "always"],
    "linebreak-style": 0,
    "import/no-extraneous-dependencies": 0,
    "lines-between-class-members": 0,
    "class-methods-use-this": 0,
    "keyword-spacing": 0,
    "no-underscore-dangle": 0,
    "function-paren-newline": 0,
    "implicit-arrow-linebreak": 0,
    "arrow-body-style": 0,
    "object-property-newline": 0
  }
}